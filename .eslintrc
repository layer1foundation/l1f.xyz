{
    "extends": [
      "next/core-web-vitals",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": ["prettier", "@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "endOfLine": "auto"
        }
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/no-explicit-any": "warn"
    },
    "root": true
  }
  